<!DOCTYPE html>
<html>
<head>
  <title>CargaBackend</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<header>
  <nav class="navbar navbar-default navbar-fixed-top nav-ayto">
  <div class="container-fluid">
    <div class="navbar-header">
      <a href="/" >
        <img id="ayto-nw-logo" src="/assets/madrid.svg">
      </a>
      <!-- <a class="navbar-brand" href="/">Carga de trabajo prototipo</a> -->
    </div>
    <div class="navbar-right">
      <a class="navbar-brand">
        <%if @current_user%>
          <p><%= @current_user.name %> <%= @current_user.last_name %> <%= @current_user.last_name_alt %></p>
          <p><%= @current_user.official_position %></p>
        <%end%>
      </a>
    </div>
  </div>
</nav>
</header>
<body>

<%= yield %>

</body>
</html>
